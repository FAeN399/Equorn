# Equorn Environment Variables

# API Keys & External Services
OPENAI_API_KEY=your_openai_key_here
POSTGRES_URI=postgresql://equorn:password@localhost:5432/loredb

# Application Settings
NODE_ENV=development
LOG_LEVEL=info
PORT=3000
API_BASE_URL=http://localhost:3000/api

# Build Options
ENABLE_GODOT_EXPORT=true
ENABLE_UNITY_EXPORT=true
ENABLE_WEB_EXPORT=true

# Feature Flags
FEATURE_LIVE_PREVIEW=true
FEATURE_COLLABORATIVE_EDITING=false
